
#OCRka

import pyocr.pyocr
import PIL.Image


tools = pyocr.pyocr.get_available_tools()[:]
print "Using '%s'" % (tools[0].get_name())
print tools[0].image_to_string(PIL.Image.open('snapshot1.png'))



#webkamera

#!/usr/bin/python2
import cv2

video = cv2.VideoCapture(0) # index of your camera
ret, snimek = video.read()
cv2.imwrite("/home/karlosss/sudoku/pokus.png", snimek)

img = cv2.imread('/home/karlosss/sudoku/pokus.png', 0)  # Reads a Gray-scale image
img2 = cv2.cvtColor(img, cv2.COLOR_GRAY2BGR)
okno = cv2.imshow("window", img2)
cv2.waitKey()